{
  "type": "library",
  "name": "zero-to-prod/data-model-adapter-json-schema4",
  "description": "An adapter for JSON Schema 4 for DataModelGenerator",
  "keywords": [
    "zero-to-prod",
    "DataModelAdapterJsonSchema4",
    "data-model-adapter-json-schema4"
  ],
  "homepage": "https://github.com/zero-to-prod/data-model-adapter-json-schema4",
  "license": "MIT",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "zero-to-prod/data-model": "^81.7",
    "zero-to-prod/data-model-helper": "^81.11.3",
    "zero-to-prod/psr4-classname": "^71.0.1",
    "zero-to-prod/json-schema4": "^1.0.1",
    "zero-to-prod/psr4-varname": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Zerotoprod\\DataModelAdapterJsonSchema4\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Factories\\": "factories/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "zero-to-prod/data-model-generator": "^0.1.10"
  },
  "suggest": {
    "zero-to-prod/data-model": "Transform Data to Type-Safe DTOs",
    "zero-to-prod/data-model-helper": "Helpers for a DataModel."
  }
}
